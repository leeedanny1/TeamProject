<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.springboot.jcmarket.domain.notice.NoticeRepository">
  
	<select id="getNoticeAll" resultType="com.springboot.jcmarket.domain.notice.Notice">
		SELECT
			notice_code,
			notice_title,
			notice_writer,
			notice_date,
			notice_count
		FROM
			notice_mst
	</select>

	
  </mapper>